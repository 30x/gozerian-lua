#!/bin/sh

exec '/usr/local/bin/luajit' -e 'package.path="/Users/shawn/.luarocks/share/lua/5.1/?.lua;/Users/shawn/.luarocks/share/lua/5.1/?/init.lua;/usr/local/share/lua/5.1/?.lua;/usr/local/share/lua/5.1/?/init.lua;"..package.path; package.cpath="/Users/shawn/.luarocks/lib/lua/5.1/?.so;/usr/local/lib/lua/5.1/?.so;"..package.cpath' -e 'local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("busted","2.0.rc11-0")' '/usr/local/lib/luarocks/rocks-5.1/busted/2.0.rc11-0/bin/busted' "$@"